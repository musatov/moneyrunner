{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "strict": false,
    "module": "commonjs",
    "target": "es2016",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "components/*": [
        "./frontend/components/*"
      ],
      "services/*": [
        "./frontend/services/*"
      ],
    }
  }
}
